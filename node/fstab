proc            /proc           proc    defaults          0       0
PARTUUID=353964cd-01  /boot           vfat    defaults          0       2
PARTUUID=353964cd-02  /               ext4    defaults,noatime  0       1
# a swapfile is not a swap partition, no line here
#   use  dphys-swapfile swap[on|off]  for that
master.screen:/data	/data	nfs	defaults,_netdev	0	0
master.screen:/apps	/apps	nfs	defaults,_netdev	0	0
master.screen:/modules	/modules	nfs	defaults,_netdev	0	0
master.screen:/etc/slurm	/etc/slurm	nfs	defaults,_netdev	0	0
